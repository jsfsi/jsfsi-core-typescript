{
  "name": "@jsfsi-core/ts-nestjs",
  "version": "1.1.29",
  "repository": {
    "type": "git",
    "url": "https://github.com/jsfsi/jsfsi-core-typescript.git"
  },
  "license": "ISC",
  "author": "",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/src/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run lint && tsc && vite build",
    "clean": "rm -rf dist && rm -rf node_modules && rm -rf coverage",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "lint": "npm run lint:fix && npm run format",
    "lint:fix": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@jsfsi-core/ts-crossplatform": "file:../ts-crossplatform",
    "@jsfsi-core/ts-nodejs": "file:../ts-nodejs",
    "@nestjs/cli": "11.0.14",
    "@nestjs/common": "11.1.11",
    "@nestjs/config": "4.0.2",
    "@nestjs/core": "11.1.11",
    "@nestjs/platform-express": "11.1.11",
    "@nestjs/typeorm": "11.0.0",
    "body-parser": "2.2.1",
    "express": "5.2.1",
    "reflect-metadata": "0.2.2",
    "typeorm": "0.3.28",
    "zod": "4.3.5"
  },
  "devDependencies": {
    "@nestjs/testing": "11.1.11",
    "@swc/core": "1.15.8",
    "@types/supertest": "6.0.3",
    "@vitest/coverage-v8": "4.0.16",
    "supertest": "7.1.4",
    "ts-node": "10.9.2",
    "unplugin-swc": "1.5.9",
    "vite": "7.3.0",
    "vite-plugin-dts": "4.5.4",
    "vitest": "4.0.16"
  },
  "peerDependenciesMeta": {
    "body-parser": {
      "optional": false
    },
    "express": {
      "optional": false
    }
  },
  "engines": {
    "node": "25.2.1",
    "npm": "11.6.2"
  }
}
