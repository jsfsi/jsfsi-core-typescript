{
  "name": "@jsfsi-core/template-authenticated-dashboard",
  "version": "1.0.0",
  "scripts": {
    "add:shadcn": "sh -c 'npx shadcn add \"$1\" --yes && if [ -d src/ui/components/ui ]; then mv src/ui/components/ui/* src/ui/components/ 2>/dev/null; rmdir src/ui/components/ui 2>/dev/null; fi' --",
    "build": "npm run lint && tsc && vite build",
    "clean": "rm -rf dist && rm -rf coverage && rm -rf node_modules",
    "deploy:production": "npm run build && gsutil -m rsync -d -r dist/ gs://template-authenticated-dashboard.jsfsi.com && gsutil -m setmeta -h \"Cache-Control:no-cache, max-age=0\" gs://template-authenticated-dashboard.jsfsi.com/index.html",
    "dev": "vite",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "lint": "npm run lint:fix && npm run format",
    "lint:fix": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "preview": "vite preview",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@hookform/resolvers": "5.2.1",
    "@jsfsi-core/ts-crossplatform": "1.0.12",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "2.1.7",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.7",
    "date-fns": "4.1.0",
    "dequal": "2.0.3",
    "firebase": "12.1.0",
    "i18next": "25.3.4",
    "i18next-browser-languagedetector": "8.2.0",
    "inversify": "7.7.1",
    "next-themes": "0.4.6",
    "react": "19.1.1",
    "react-day-picker": "9.8.1",
    "react-dom": "19.1.1",
    "react-hook-form": "7.62.0",
    "react-i18next": "15.6.1",
    "react-router-dom": "7.8.0",
    "reflect-metadata": "0.2.2",
    "shadcn": "2.10.0",
    "sonner": "2.0.7",
    "zod": "4.0.17"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "4.1.11",
    "@testing-library/dom": "10.4.1",
    "@testing-library/jest-dom": "6.6.4",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@types/react": "19.1.9",
    "@types/react-dom": "19.1.7",
    "@types/testing-library__dom": "7.5.0",
    "@vitejs/plugin-react": "5.0.0",
    "@vitest/coverage-v8": "3.2.4",
    "autoprefixer": "10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "2.1.1",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.20",
    "jsdom": "26.1.0",
    "lucide-react": "^0.539.0",
    "tailwind-merge": "3.3.1",
    "tailwindcss": "4.1.11",
    "tw-animate-css": "1.3.6",
    "vite": "7.1.1",
    "vite-plugin-compression": "0.5.1",
    "vitest": "3.2.4"
  }
}
