{
  "name": "@jsfsi-core/template-authenticated-dashboard",
  "version": "1.0.0",
  "scripts": {
    "add:shadcn": "sh -c 'npx shadcn add \"$1\" --yes && if [ -d src/ui/components/ui ]; then mv src/ui/components/ui/* src/ui/components/ 2>/dev/null; rmdir src/ui/components/ui 2>/dev/null; fi' --",
    "build": "npm run lint && tsc && vite build",
    "clean": "rm -rf dist && rm -rf coverage && rm -rf node_modules",
    "deploy:production": "npm run build && gsutil -m rsync -d -r dist/ gs://template-authenticated-dashboard.jsfsi.com && gsutil -m setmeta -h \"Cache-Control:no-cache, max-age=0\" gs://template-authenticated-dashboard.jsfsi.com/index.html",
    "dev": "vite",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "lint": "npm run lint:fix && npm run format",
    "lint:fix": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "preview": "vite preview",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@hookform/resolvers": "5.2.2",
    "@jsfsi-core/ts-crossplatform": "file:../ts-crossplatform",
    "@radix-ui/react-avatar": "1.1.11",
    "@radix-ui/react-dialog": "1.1.15",
    "@radix-ui/react-dropdown-menu": "2.1.16",
    "@radix-ui/react-label": "2.1.8",
    "@radix-ui/react-separator": "1.1.8",
    "@radix-ui/react-slot": "1.2.4",
    "@radix-ui/react-tooltip": "1.2.8",
    "date-fns": "4.1.0",
    "dequal": "2.0.3",
    "firebase": "12.8.0",
    "i18next": "25.8.0",
    "i18next-browser-languagedetector": "8.2.0",
    "inversify": "7.11.0",
    "next-themes": "0.4.6",
    "react": "19.2.3",
    "react-day-picker": "9.13.0",
    "react-dom": "19.2.3",
    "react-hook-form": "7.71.1",
    "react-i18next": "16.5.3",
    "react-router-dom": "7.13.0",
    "reflect-metadata": "0.2.2",
    "shadcn": "3.7.0",
    "sonner": "2.0.7",
    "zod": "4.3.6"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "4.1.18",
    "@testing-library/dom": "10.4.1",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.2",
    "@testing-library/user-event": "14.6.1",
    "@types/react": "19.2.9",
    "@types/react-dom": "19.2.3",
    "@types/testing-library__dom": "7.5.0",
    "@vitejs/plugin-react": "5.1.2",
    "@vitest/coverage-v8": "4.0.18",
    "autoprefixer": "10.4.23",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-react-refresh": "0.4.26",
    "jsdom": "27.4.0",
    "lucide-react": "0.563.0",
    "tailwind-merge": "3.4.0",
    "tailwindcss": "4.1.18",
    "tw-animate-css": "1.4.0",
    "vite": "7.3.1",
    "vite-plugin-compression": "0.5.1",
    "vitest": "4.0.18"
  }
}
